<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Shop_FifthUI_Order">

    <ImageButton
        android:id="@+id/placesbtn"
        android:layout_width="93dp"
        android:layout_height="70dp"
        android:backgroundTint="@color/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/p9" />

    <ImageButton
        android:id="@+id/hotelsbtn"
        android:layout_width="90dp"
        android:layout_height="70dp"
        android:backgroundTint="@color/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/p10" />

    <ImageButton
        android:id="@+id/shopbtn"
        android:layout_width="90dp"
        android:layout_height="70dp"
        android:backgroundTint="@color/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/p12" />

    <ImageButton
        android:id="@+id/homebtn"
        android:layout_width="93dp"
        android:layout_height="70dp"
        android:backgroundTint="@color/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/p8" />

    <ImageButton
        android:id="@+id/guidesbtn"
        android:layout_width="90dp"
        android:layout_height="70dp"
        android:backgroundTint="@color/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.747"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/p11" />

    <ImageView
        android:id="@+id/imgUi5"
        android:layout_width="413dp"
        android:layout_height="245dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/download" />

    <TableRow
        android:id="@+id/ui_backWh"
        android:layout_width="244dp"
        android:layout_height="60dp"
        android:background="@android:drawable/editbox_dropdown_light_frame"
        android:backgroundTint="@android:color/background_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.145">

        <TextView
            android:id="@+id/ui5_topic"
            android:layout_width="196dp"
            android:layout_height="75dp"
            android:fontFamily="sans-serif-black"
            android:text="@string/ui5_topic"
            android:textAlignment="textEnd"
            android:textSize="30sp" />
    </TableRow>


    <EditText
        android:id="@+id/Label1_cat"
        android:layout_width="341dp"
        android:layout_height="71dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        android:text="@string/Label1_cat"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/hotelsbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgUi5"
        app:layout_constraintVertical_bias="0.371" />

    <EditText
        android:id="@+id/label2_item"
        android:layout_width="342dp"
        android:layout_height="59dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        android:text="@string/label2_item"
        app:layout_constraintBottom_toBottomOf="@+id/hotelsbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.468" />

    <ImageView
        android:id="@+id/uploadlabelim"
        android:layout_width="102dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toBottomOf="@+id/hotelsbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.883"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgUi5"
        app:layout_constraintVertical_bias="0.545"
        app:srcCompat="@android:drawable/ic_menu_upload" />

    <TextView
        android:id="@+id/label3_im"
        android:layout_width="74dp"
        android:layout_height="44dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/label3_im"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/hotelsbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.103"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.567" />

    <EditText
        android:id="@+id/label4_pr"
        android:layout_width="346dp"
        android:layout_height="47dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        android:text="@string/label4_pr"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.634" />

    <EditText
        android:id="@+id/label5_q"
        android:layout_width="344dp"
        android:layout_height="55dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        android:text="@string/label5_q"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.711" />

    <EditText
        android:id="@+id/label6_si"
        android:layout_width="352dp"
        android:layout_height="52dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        android:text="@string/label6_si"
        app:layout_constraintBottom_toBottomOf="@+id/placesbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.782" />

    <Button
        android:id="@+id/ui5_butAdd"
        android:layout_width="143dp"
        android:layout_height="52dp"
        android:backgroundTint="@color/b"
        android:fontFamily="sans-serif-black"
        android:text="@string/ui5_butAdd"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/hotelsbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.886" />

    <Button
        android:id="@+id/ui5_canBtn"
        android:layout_width="130dp"
        android:layout_height="55dp"
        android:backgroundTint="@color/colorPrimaryDark"
        android:fontFamily="sans-serif-black"
        android:text="@string/ui5_canBtn"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.903"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgUi5"
        app:layout_constraintVertical_bias="0.886" />
</androidx.constraintlayout.widget.ConstraintLayout>